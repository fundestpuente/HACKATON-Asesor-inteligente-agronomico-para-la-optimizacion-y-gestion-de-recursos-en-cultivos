{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-native",
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
